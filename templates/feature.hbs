import React, { useState, useEffect } from 'react';
import styled from 'styled-components';

const {{name}} = () => {
  const [name, setName] = useState('');

  useEffect(() => {
    setName('{{name}}');
  }, []);

  return (
    <Container>
      <Text>{name}</Text>
    </Container>
  );
};

const Container = styled.div`
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
`;

const Text = styled.div``;

export default {{name}};
